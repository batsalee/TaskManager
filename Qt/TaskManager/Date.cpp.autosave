#include "Date.h"

Date::Date()
{
    getTodaysDate();
}

Date::Date(int y, int m, int d)
{
    year = y;
    month = m;
    day = d;
}

/////////////////////////////////////////////////////////////////////////

void Date::getTodaysDate()
{
    QDate currentDate = QDate::currentDate();
    year = currentDate.year();
    month = currentDate.month();
    da = currentDate.day();
    
}

void Date::setDate(int y, int m, int d)
{
    year = y;
    month = m;
    day = d;
}
